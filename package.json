{
    "name": "ci-platform-backend",
    "version": "1.0.0",
    "description": "RestApis for CI platform",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "dev": "nodemon src/index.js",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix .",
        "migrate:up": "npx sequelize-cli db:migrate",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "seed:up":"npx sequelize-cli db:seed:all",
        "seed:undo":"npx sequelize-cli db:seed:undo"
    },
    "author": "Ashish Chauhan",
    "license": "ISC",
    "devDependencies": {
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "nodemon": "^3.0.2",
        "prettier": "^3.1.1",
        "sequelize-cli": "^6.6.2"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.35.2"
    }
}
